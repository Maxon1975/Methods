{
  "parser": "@babel/eslint-parser",
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "import/no-named-as-default-member": "off",
    "import/no-named-as-default": "off",
    "max-len": "off",
    "no-param-reassign": "off",
    "no-mixed-spaces-and-tabs": "off",
    "no-tabs": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
